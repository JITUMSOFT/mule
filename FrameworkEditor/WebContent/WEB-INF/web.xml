<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>FrameworkEditor</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>MainPage.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>HandleConfig</servlet-name>
    <servlet-class>com.framework.config.HandleConfig</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HandleConfig</servlet-name>
    <url-pattern>/HandleConfig</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>FrameworkServlet</servlet-name>
    <servlet-class>com.framework.config.FrameworkServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrameworkServlet</servlet-name>
    <url-pattern>/FrameworkServlet</url-pattern>
  </servlet-mapping>
</web-app>